{
  "$schema": "https://json-schema.org/draft-07/schema",
  "id": "https://jacklehamster.github.io/dok-schema/process/actions/output.json",
  "description": "An action to send output",
  "allOf": [{
    "$ref": "action.json"
  }],
  "properties": {
    "outputStream": {
      "$ref": "../expressions/string-expression.json",
      "enum": ["dialog","console"]
    },
    "output": {
      "type": "array",
      "items": {
        "$ref": "../expressions/expression.json"
      },
      "minItems": 1
    }
  },
  "required": ["output", "outputStream"],
  "additionalProperties": false
}
